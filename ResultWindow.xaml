<Window x:Class="ConnectionApp.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConnectionApp"
        mc:Ignorable="d"
        Title="ResultWindow" Height="300" Width="400">
    <Grid>
        <TextBlock x:Name="resultTextBlock" HorizontalAlignment="Center" Margin="0,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Text="test" Height="60"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="Center" Margin="0,200,0,0" VerticalAlignment="Top" Click="buttonOK_Click"/>

    </Grid>
</Window>
